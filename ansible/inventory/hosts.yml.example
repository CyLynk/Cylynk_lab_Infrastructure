---
# Example inventory file
# Copy this to hosts.yml and update with your Terraform outputs
all:
  children:
    guacamole:
      hosts:
        guacamole-dev:
          ansible_host: 1.2.3.4  # Replace with module.guacamole.public_ip from Terraform
          ansible_user: ubuntu
          ansible_ssh_private_key_file: ~/.ssh/cylynk-lab-keypair
          environment: dev
          project_name: cyberlab
          domain_name: ""
          admin_email: "dev-admin@yourdomain.com"
          enable_lets_encrypt: false

